    "test:e2e:ui": "playwright test --ui",
    "test:workflow": "tsx scripts/test-full-workflow.ts",
    "test:integration": "tsx scripts/test-complete-integration.ts",
    "type-check": "tsc --noEmit",
    "init-db": "tsx lib/database/init-db.ts",
    "setup-db": "tsx lib/database/setup-db.ts",
    "db:start": "docker-compose up -d postgres",
    "db:stop": "docker-compose down",
    "db:reset": "docker-compose down -v && docker-compose up -d postgres && npm run init-db",
    "db:admin": "docker-compose --profile tools up pgadmin"
